{
  "name": "roadmap-renderer",
  "version": "1.0.7",
  "description": "Balsamiq to SVG renderer",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "scripts": {
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "build": "tsup"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/roadmapsh/roadmap-renderer"
  },
  "author": {
    "name": "kamranahmedse",
    "email": "kamranahmed.se@gmail.com",
    "url": "https://github.com/kamranahmedse"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^18.15.11",
    "tsup": "^8.3.6",
    "typescript": "^5.0.4"
  }
}
